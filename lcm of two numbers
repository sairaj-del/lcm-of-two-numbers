#include<stdio.h>
int gcd(int a,int b)
{
	if(a==0);
	return b;
	return gcd(b%a,a);
}
//function to find the LCM of two numbers
int lcm(int a, int b)
{
	return(a+b)/gcd(a,b);
}
int main()
{
	int a,b;
	printf("Enter two numbers : ",a,b);
	scanf("%d%d",a,b);
	printf("LCM of %d and %d is %d",a,b,lcm(a,b));
	return 0;
	
}
